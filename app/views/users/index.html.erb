<div class="container">
  <h1>全てのユーザー</h1>
  <div class="row">
    <div class="form-inline">
      <div class="col-md-4 col-md-offset-8",style="display:inline;">
        <%= form_with(url:users_path, method: :get, local:true) do |f| %>
          <span><%= f.label:search,"ユーザー名検索" %></span>
          <span style="display:inline;"><%= f.text_field:search, class: "form-control" %><%= f.submit "検索", class: "btn btn-primary" %></span>
        <% end %>
      </div>
    </div>
  </div>
</div>